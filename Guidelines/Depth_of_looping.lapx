STATE before
    CUR_DEPTH = 1
    myl = []
    RES = []
END_STATE

STATE iteration
    global myl
    global RES
    global CUR_DEPTH
    if ITERATION in ["for","while"]:
        temp = []
        getTokens(items,temp)
        t = 0
        for i in STATEMENTS:
            if 'while' in i[0:5] or 'for' in i[0:3]:
                t+=1
        if t>0:
            CUR_DEPTH+=1
            myl.append(t)
            if CUR_DEPTH > 2:
                RES.append(LINE_NO)
        else:
            while(len(myl)):
                myl[-1]-=1
                if(myl[-1]==0):
                    CUR_DEPTH-=1
                    myl.pop()
                else:
                    break
END_STATE

